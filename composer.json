{
    "name": "oryzone/media-storage-bundle",
    "type": "symfony-bundle",
    "description": "Simple media files storage for symfony2",
    "keywords": ["media storage", "storage","media", "files", "images", "videos", "documents"],
    "homepage": "https://github.com/Oryzone/OryzoneMediaStorageBundle",
    "license": "MIT",
    "authors": [
    {
        "name": "Luciano Mammino",
        "email": "lmammino@oryzone.com",
        "homepage": "http://oryzone.com",
        "role": "Developer"
    },
    {
        "name": "Andrea Mangano",
        "email": "amangano@oryzone.com",
        "homepage": "http://oryzone.com",
        "role": "Designer"
    }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lonelyplanet/TEOryzoneMediaStorage"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "oryzone/media-storage": "dev-fixdependencies",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "knplabs/knp-gaufrette-bundle": "dev-master#171844b400e50d66f6fab6aa7aea820407fc74a3"
    },
    "suggest": {
        "avalanche123/imagine-bundle": "*",
        "sensio/buzz-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Oryzone\\Bundle\\MediaStorageBundle": "" }
    },
    "target-dir": "Oryzone/Bundle/MediaStorageBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
